// get all packages
    get http://127.0.0.1:8000/api/package/sel-all

// get one package
get http://127.0.0.1:8000/api/package/sel-one/id


// insert package
    post http://127.0.0.1:8000/api/package/insert
    token: yes
    example:
    {
    "name":"aaa",
    "desc": "aaa",
    "price": "111",
    "card_qty": "111",
    "package_equipment":[{
        "equipment_id":1,
        "package_qty":2,
        "equipment_name":"aaa"
    },
    {
        "equipment_id":2,
        "package_qty":2,
        "equipment_name":"aaa"
    }],
    "package_food":[{
        "food_id":1,
        "food_name":"aaa"
    },
    {
        "food_id":3,
        "food_name":"aaa"
    }]
}

// update package
    post http://127.0.0.1:8000/api/package/update/{id}
    token: yes
 example:
    {
    "name":"aaa",
    "desc": "aaa",
    "price": "111",
    "card_qty": "111",
    "package_equipment":[{
        "equipment_id":1,
        "package_qty":2,
        "equipment_name":"aaa"
    },
    {
        "equipment_id":2,
        "package_qty":2,
        "equipment_name":"aaa"
    }],
    "package_food":[{
        "food_id":1,
        "food_name":"aaa"
    },
    {
        "food_id":3,
        "food_name":"aaa"
    }]
}


// delete package
    delete http://127.0.0.1:8000/api/package/delete/{id}
    token: yes